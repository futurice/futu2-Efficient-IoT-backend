{
  "name": "futu_2-backend-server",
  "version": "0.0.1",
  "description": "Backend for handling communication between clients and sensors",
  "main": "futu_2-backend-server",
  "scripts": {
    "babel-node": "./node_modules/node-babel/node_modules/.bin/babel-node",
    "jscs": "$(npm bin)/jscs */**.js || exit 0",
    "lint": "$(npm bin)/eslint */**.js || exit 0",
    "start": "$(npm bin)/nodemon --exec npm run babel-node -- ./bin/www",
    "dev-run": "npm run lint & npm run jscs & $(npm bin)/nodemon --exec npm run babel-node -- ./bin/www",
    "watch": "npm run dev-run"
  },
  "keywords": [
    "IoT"
  ],
  "license": "ISC",
  "dependencies": {
    "babel": "^6.1.18",
    "body-parser": "^1.14.1",
    "debug": "^2.2.0",
    "ejs": "^2.3.4",
    "eslint": "^1.9.0",
    "express": "^4.13.3",
    "morgan": "^1.6.1",
    "node-babel": "^0.1.2",
    "nodemon": "^1.8.1",
    "rx": "^4.0.7",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.3.7"
  },
  "devDependencies": {
    "jscs": "^2.5.1"
  }
}
